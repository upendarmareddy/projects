package com.tcs;

public class Cart {
	private int cartId;
	MenuItem[] items;
	public Cart() {
		super();
	}
	public Cart(int cartId, MenuItem[] items) {
		super();
		this.cartId = cartId;
		this.items = items;
	}
	public int getCartId() {
		return cartId;
	}
	public void setCartId(int cartId) {
		this.cartId = cartId;
	}
	public MenuItem[] getItem() {
		return items;
	}
	public void setItem(MenuItem[] items) {
		this.items = items;
	}
}
