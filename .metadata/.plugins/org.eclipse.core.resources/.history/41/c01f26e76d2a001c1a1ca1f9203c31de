package com.tcs;

public class Cart {
	private int cartId;
	MenuItem item;
	public Cart() {
		super();
	}
	public Cart(int cartId, MenuItem item) {
		super();
		this.cartId = cartId;
		this.item = item;
	}
	public int getCartId() {
		return cartId;
	}
	public void setCartId(int cartId) {
		this.cartId = cartId;
	}
	public MenuItem getItem() {
		return item;
	}
	public void setItem(MenuItem item) {
		this.item = item;
	}
	
	

}
